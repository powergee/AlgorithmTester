{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Build ExecutionBase",
			"command": "/usr/bin/g++",
			"args": [
				"-g",
				"*.cpp",
				"-o",
				"${workspaceFolder}/ExecutionBase/main.exe"
			],
			"options": {
				"cwd": "${workspaceFolder}/ExecutionBase"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
	]
}